    <formulas>
        <description>This formula will add the current Work Type Group ID to the list of Work Type Groups.</description>
        <name>CurrentWorkTypeGroupID</name>
        <dataType>String</dataType>
        <expression>&quot;,&quot;+{!Loop_All_Work_Type_Group_Members.WorkTypeGroupId}</expression>
    </formulas>
    <formulas>
        <description>Formula to remove the initial concatenate comma</description>
        <name>RemoveComma</name>
        <dataType>String</dataType>
        <expression>TRIM(right({!AllWorkTypeIDs}, (LEN( {!AllWorkTypeIDs}) - 1)))</expression>
    </formulas>
    <formulas>
        <description>The BenefitID field in the Session only includes the first 15 characters. We need to match these to find the correct Session.</description>
        <name>TruncatedBenefitID</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT( {!Get_Newly_Created_Service_Appointment.WorkType.Benefit__c} , 15))</expression>
    </formulas>
